{
  "scripts": {
    "dev": "cross-env COMMIT=$(git log -1 --format=\"%h\") node index",
    "start": "cross-env NODE_ENV=production COMMIT=$(git log -1 --format=\"%h\") node index",
    "build": "rm -rf dist && mkdir dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production COMMIT=$(git log -1 --format=\"%h\") webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production COMMIT=$(git log -1 --format=\"%h\") webpack --config build/webpack.server.config.js --progress --hide-modules",
    "lint": "eslint $(find . \\( -name \"*.js\" -o -name \"*.vue\" \\) -not \\( -path \"./node_modules/*\" -o -path \"./dist/*\" -o -name \".*\" \\))",
    "test": "cross-env NODE_ENV=test nyc mocha ./test --require babel-register --require babel-polyfill --recursive -u tdd --timeout 999999 --colors --exit",
    "cover": "nyc report --reporter=text-lcov | coveralls && rm -rf ./coverage && rm -rf ./.nyc_output"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "acorn": "^5.3.0",
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "bufferutil": "^3.0.3",
    "eslint": "^4.15.0",
    "express": "^4.16.2",
    "highlight.js": "^9.12.0",
    "markdown-it": "^8.4.0",
    "memory-fs": "^0.4.1",
    "mongodb": "^3.0.1",
    "multer": "^1.3.0",
    "pug": "^2.0.0-rc.4",
    "regenerator-runtime": "^0.11.1",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "utf-8-validate": "^4.0.0",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.13",
    "vuex": "^3.0.1",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.8",
    "eslint": "^4.15.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.1.0",
    "file-loader": "^1.1.6",
    "istanbul": "^0.4.5",
    "mocha": "^4.1.0",
    "node-sass": "^4.7.2",
    "nyc": "^11.4.1",
    "sass-loader": "^6.0.6",
    "supertest": "^3.0.0",
    "vue-loader": "^13.7.0",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.1",
    "webpack-hot-middleware": "^2.21.0"
  }
}
